{
  "name": "printer3d-frontend",
  "version": "0.19.0-12",
  "scripts": {
    "cucumber": "$(npm bin)/cucumber-js ./cypress/**/*.feature --no-strict --tags @F3D25.01",
    "docker:start": "docker-compose -f ./support/printer3d-docker-frontend/docker-compose.yml up & ",
    "docker:stop": "docker-compose -f ./support/printer3d-docker-frontend/docker-compose.yml down ",
    "app:banner:generate": "figlet `./versionGenerator.sh` | sed 's/^/| /' > app-banner.txt",
    "app:banner:copy": "cp ./app-banner.txt ../Printer3D-backend/src/main/resources",
    "app:banner": "npm run app:banner:generate &&  npm run app:banner:copy",
    "build:prod": "ng build --aot --configuration=production",
    "test:prod": "ng test --code-coverage --watch=false --no-progress --browsers=ChromeHeadlessCI",
    "sonar:prod": "node sonar-scan.js ",
    "check:deploy": "npm run build:prod && npm run test:prod && cypress run",
    "cypress:record": "cypress run --record --key 9eebb605-a163-4da0-bb36-6af73faddf1b",
    "acceptance:cypress:D02": "cypress run --spec ./cypress/acceptance/D3D02-Dock.feature",
    "acceptance:cypress:D04": "cypress run --spec ./cypress/acceptance/D3D04-NewPart.feature",
    "acceptance:cypress:D05": "cypress run --spec ./cypress/acceptance/D3D05-NewCoil.feature",
    "acceptance:cypress:D06": "cypress run --spec ./cypress/acceptance/D3D06-Jobs.feature",
    "acceptance:cypress:D08": "cypress run --spec ./cypress/acceptance/D3D08-NewModel.feature",
    "acceptance:cypress:D09": "cypress run --spec ./cypress/acceptance/D3D09-NewRequest.feature",
    "acceptance:cypress:D10": "cypress run --spec ./cypress/acceptance/D3D10-Coils.feature",
    "acceptance:cypress:D11": "cypress run --spec ./cypress/acceptance/D3D11-OpenRequests.feature",
    "acceptance:cypress:D12": "cypress run --spec ./cypress/acceptance/D3D12-RequestDetails.feature",
    "acceptance:cypress:D13": "cypress run --spec ./cypress/acceptance/D3D13-Machine.feature",
    "acceptance:cypress:D14": "cypress run --spec ./cypress/acceptance/D3D14-MachineJobCompletion.feature",
    "acceptance:cypress:D15": "cypress run --spec ./cypress/acceptance/D3D15-RequestableItems.feature",
    "acceptance:cypress:D18": "cypress run --spec ./cypress/acceptance/D3D18-EditPart.feature",
    "acceptance:cypress:D19": "cypress run --spec ./cypress/acceptance/D3D19-AvailableStocks.feature",
    "acceptance:cypress:D20": "cypress run --spec ./cypress/acceptance/D3D20-InventoryContents.feature",
    "acceptance:cypress:F21": "cypress run --spec ./cypress/acceptance/F3D21-BillingChart.feature",
    "acceptance:cypress:F22": "cypress run --spec ./cypress/acceptance/F3D22-ModelEditing.feature",
    "acceptance:cypress:F23": "cypress run --spec ./cypress/acceptance/F3D23-PatchNotes.feature",
    "acceptance:cypress": "npm run start:dev & wait-on http://localhost:5100 && npm run cypress:run ",
    "serve:integration": "export NODE_ENV=integration && export PORT=5102 && npm run build:dev && npm start",
    "analyzer": "ng build --aot --configuration=stage --stats-json && webpack-bundle-analyzer ./dist/Printer3D-frontend/stats-es2015.json",
    "test:ci": "ng test --watch=false --no-progress --browsers=ChromeHeadlessCI  --code-coverage",
    "npm:bit:configuration": "npm config set @bit:registry https://node.bitsrc.io",
    "npm:install:cli": "npm install @angular/cli@9.1.6 @angular-devkit/build-angular@0.901.6 @angular/compiler-cli@9.1.7 typescript@3.8.3 --save-prod ",
    "heroku-prebuild": "npm run npm:bit:configuration && npm run npm:install:cli ",
    "heroku-postbuild": "ng build --configuration=${NODE_ENV}",
    "serve:local-old": "npm run tag && npm run build:dev && . ./config/.env.local ; npm start",
    "build:stage": "ng build --aot --configuration=stage",
    "build:develop": "ng build --configuration=develop",
    "acceptance:start": "npm run env:local && . ./config/.env.local && npm run tag && npm run build:develop && export NODE_ENV=acceptance && export PORT=5103 && npm start",
    "cypress:open": "cypress open ",
    "version:generate": "gitversion /showvariable AssemblySemFileVer > .version && cat .version",
    "version:update": "npm run version:generate && npm version --allow-same-version `cat .version`",
    "tag": "npm run version:update && echo $MODULE_CODE / `gitversion /showvariable AssemblySemFileVer` | figlet > .app-banner && cat .app-banner",
    "env:local": "NODE_ENV=local . ./.deploy/generate.env.sh",
    "env:develop": "NODE_ENV=develop . ./.deploy/generate.env.sh",
    "env:production": "NODE_ENV=production . ./.deploy/generate.env.sh",
    "check": "dotenv -p NODE_ENV",
    "serve:local": "npm run env:local && npm run tag && ng serve --progress --proxy-config proxy.conf.json --port 5101",
    "test:dev": "NODE_OPTIONS=--openssl-legacy-provider ng test --code-coverage --watch --progress ",
    "acceptance:prepare": "dotenv -- ./support/simulator_backend.sh start",
    "acceptance:simulator": "bash ./support/simulator-backend.sh start &",
    "acceptance:cypress:D01": "cypress run --spec ./cypress/acceptance/D3D01-Dashboard.feature"
  },
  "private": true,
  "engines": {
    "node": "18.20.4",
    "npm": "10.7.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "@angular/animations": "12.2.17",
    "@angular/cdk": "12.2.13",
    "@angular/common": "12.2.17",
    "@angular/compiler": "12.2.17",
    "@angular/core": "12.2.17",
    "@angular/forms": "12.2.17",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "12.2.17",
    "@angular/platform-browser-dynamic": "12.2.17",
    "@angular/router": "12.2.17",
    "@bahmutov/add-typescript-to-cypress": "2.1.2",
    "@bit/innovative.innovative-core.interfaces": "^0.1.2",
    "@bit/innovative.innovative-core.isolation-service": "^0.1.1",
    "@bit/innovative.innovative-core.testing-support": "^0.1.1",
    "@swimlane/ngx-charts": "^20.5.0",
    "@types/uuid": "10.0.0",
    "compression": "1.7.4",
    "config": "^3.3.12",
    "dotenv-cli": "^7.4.2",
    "env-cmd": "^10.1.0",
    "express": "^4.21.1",
    "express-http-proxy": "^2.1.1",
    "jwt-decode": "^4.0.0",
    "ng-drag-drop": "5.0.0",
    "ngx-toastr": "^14.3.0",
    "ngx-webstorage-service": "^4.1.0",
    "npm": "6.14.5",
    "path": "0.12.7",
    "request": "2.88.2",
    "rxjs": "6.6.0",
    "tslib": "2.0.0",
    "uuid": "10.0.0",
    "wait-on": "5.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.18",
    "@angular/cli": "12.2.18",
    "@angular/compiler-cli": "12.2.17",
    "@cypress/code-coverage": "3.7.2",
    "@cypress/webpack-preprocessor": "5.2.1",
    "@types/chai": "4.2.11",
    "@types/cucumber": "6.0.1",
    "@types/cypress-cucumber-preprocessor": "1.14.1",
    "@types/jasmine": "3.6.0",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.11.1",
    "chai": "4.2.0",
    "chromedriver": "81.0.0",
    "cucumber": "6.0.5",
    "cypress": "6.8.0",
    "cypress-cucumber-preprocessor": "2.3.1",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "5.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.2",
    "karma-jasmine": "4.0.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "nyc": "15.0.1",
    "protractor": "7.0.0",
    "ts-node": "8.3.0",
    "tslint": "6.1.0",
    "typescript": "4.3.5"
  }
}
