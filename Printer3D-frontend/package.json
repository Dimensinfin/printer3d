{
    "name": "printer3d-frontend",
    "version": "v0.11.1",
    "backversion": "<0.6.0",
    "scripts": {
        "cucumber": "$(npm bin)/cucumber-js ./cypress/**/*.feature --no-strict --tags @D3D09.11",
        "docker:start": "docker-compose -f ./support/docker/docker-compose.yml up & ",
        "docker:stop": "docker-compose -f ./support/docker/docker-compose.yml down ",
        "serve:dev": "ng serve --progress --proxy-config proxy.conf.json --port 5100",
        "build:dev": "ng build ",
        "build:stage": "ng build --aot --configuration=stage",
        "build:prod": "ng build --aot --configuration=production",
        "test:dev": "ng test --code-coverage --watch --progress ",
        "test:prod": "ng test --watch=false --no-progress --browsers=ChromeHeadlessCI",
        "check:deploy": "npm run build:prod && npm run test:prod && cypress run",
        "cypress:run": "cypress run ",
        "cypress:record": "cypress run --record --key 9eebb605-a163-4da0-bb36-6af73faddf1b",
        "acceptance:simulator": "bash ./support/simulator-backend.sh start &",
        "acceptance:ready": "export NODE_ENV=integration && export PORT=5100 && npm start &",
        "acceptance:cypress:D01": "cypress run --spec ./cypress/acceptance/D3D01-Dashboard.feature",
        "acceptance:cypress:D04": "cypress run --spec ./cypress/acceptance/D3D04-NewPart.feature",
        "acceptance:cypress:D06": "cypress run --spec ./cypress/acceptance/D3D06-Jobs.feature",
        "acceptance:cypress:D07": "cypress run --spec ./cypress/acceptance/D3D07-Inventory.feature",
        "acceptance:cypress:D08": "cypress run --spec ./cypress/acceptance/D3D08-NewModel.feature",
        "acceptance:cypress:D09": "cypress run --spec ./cypress/acceptance/D3D09-NewRequest.feature",
        "acceptance:cypress:D10": "cypress run --spec ./cypress/acceptance/D3D10-Coils.feature",
        "acceptance:cypress:D11": "cypress run --spec ./cypress/acceptance/D3D11-OpenRequests.feature",
        "acceptance:cypress": "npm run start:dev & wait-on http://localhost:5100 && npm run cypress:run ",
        "acceptance:cypress:batchA": "npm run start:dev & wait-on http://localhost:5100 && cypress run --spec \"./cypress/acceptance/D3D0*.feature\"",
        "acceptance:cypress:batchB": "npm run start:dev & wait-on http://localhost:5100 && cypress run --spec \"./cypress/acceptance/D3D1*.feature\"",
        "integration:cypress:batchA": "npm run start:dev & wait-on http://localhost:5100 && cypress run --spec \"./cypress/integration/D3D*.feature\"",
        "heroku-postbuild": "ng build --configuration=${NODE_ENV}",
        "serve:integration": "export NODE_ENV=integration && export PORT=5150 && npm run build:dev && npm start",
        "analyzer": "ng build --aot --configuration=stage --stats-json && webpack-bundle-analyzer ./dist/Printer3D-frontend/stats-es2015.json"
    },
    "private": true,
    "engines": {
        "node": "12.16.3",
        "npm": "6.14.4"
    },
    "cypress-cucumber-preprocessor": {
        "nonGlobalStepDefinitions": true,
        "cucumberJson": {
            "generate": true,
            "outputFolder": "cypress/cucumber-json",
            "filePrefix": "",
            "fileSuffix": ".cucumber"
        }
    },
    "dependencies": {
        "@angular/animations": "9.1.7",
        "@angular/cdk": "9.2.4",
        "@angular/common": "9.1.7",
        "@angular/compiler": "9.1.7",
        "@angular/core": "9.1.7",
        "@angular/forms": "9.1.7",
        "@angular/material": "9.2.4",
        "@angular/platform-browser": "9.1.7",
        "@angular/platform-browser-dynamic": "9.1.7",
        "@angular/router": "9.1.7",
        "@bahmutov/add-typescript-to-cypress": "2.1.2",
        "@swimlane/ngx-charts": "^14.0.0",
        "@types/uuid": "7.0.3",
        "angular-rollbar": "1.0.0",
        "compression": "1.7.4",
        "config": "3.3.1",
        "express": "4.17.1",
        "express-http-proxy": "1.6.0",
        "jwt-decode": "2.2.0",
        "ng-drag-drop": "5.0.0",
        "ngx-toastr": "12.0.1",
        "ngx-webstorage-service": "4.1.0",
        "npm": "6.14.5",
        "path": "0.12.7",
        "request": "2.88.2",
        "rollbar": "2.18.0",
        "rxjs": "6.5.4",
        "tslib": "1.10.0",
        "uuid": "8.0.0",
        "wait-on": "5.0.0",
        "zone.js": "0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.901.6",
        "@angular/cli": "9.1.6",
        "@angular/compiler-cli": "9.1.7",
        "@cypress/code-coverage": "3.7.2",
        "@cypress/webpack-preprocessor": "5.2.1",
        "@types/chai": "4.2.11",
        "@types/cucumber": "6.0.1",
        "@types/cypress-cucumber-preprocessor": "1.14.1",
        "@types/jasmine": "3.5.0",
        "@types/jasminewd2": "2.0.3",
        "@types/node": "12.11.1",
        "chai": "4.2.0",
        "chromedriver": "81.0.0",
        "codelyzer": "5.1.2",
        "cucumber": "6.0.5",
        "cypress": "^4.10.0",
        "cypress-cucumber-preprocessor": "2.3.1",
        "jasmine-core": "3.5.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "5.0.0",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "2.1.0",
        "karma-jasmine": "3.0.1",
        "karma-jasmine-html-reporter": "1.4.2",
        "nyc": "15.0.1",
        "protractor": "5.4.3",
        "ts-node": "8.3.0",
        "tslint": "6.1.0",
        "typescript": "3.8.3"
    }
}